{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": "find",
            "keys": "ctrl+f"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "vertical",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+x"
        },
        {
            "command": "tabSearch",
            "keys": "ctrl+shift+space"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "horizontal",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+s"
        },
        {
            "command": 
            {
                "action": "openSettings"
            },
            "keys": "ctrl+/"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "ctrl+k"
        },
        {
            "command": 
            {
                "action": "newTab"
            },
            "keys": "alt+t"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "ctrl+j"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "ctrl+l"
        },
        {
            "command": 
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "ctrl+h"
        }
    ],
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultProfile": "{2c4de342-38b7-51cf-b940-2309a097f518}",
    "initialCols": 60,
    "initialRows": 15,
    "profiles": 
    {
        "defaults": 
        {
            "acrylicOpacity": 0.43,
            "closeOnExit": "graceful",
            "colorScheme": "judy",
            "cursorColor": "#FF00FF",
            "cursorHeight": 50,
            "cursorShape": "emptyBox",
            "font": 
            {
                "face": "CaskaydiaCovePL Nerd Font",
                "size": 13,
                "weight": "extra-light"
            },
            "historySize": 9001,
            "padding": "0",
            "tabColor": "#553355",
            "useAcrylic": true
        },
        "list": 
        [
            {
                "guid": "{2c4de342-38b7-51cf-b940-2309a097f518}",
