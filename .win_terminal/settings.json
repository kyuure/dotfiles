{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions":
    [
        {
            "command": "find",
            "keys": "ctrl+f"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "vertical",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+x"
        },
        {
            "command": "tabSearch",
            "keys": "ctrl+shift+space"
        },
        {
            "command":
            {
                "action": "splitPane",
                "split": "horizontal",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+s"
        },
        {
            "command": "unbound",
            "keys": "ctrl+comma"
        },
        {
            "command": "unbound",
            "keys": "ctrl+shift+t"
        },
        {
            "command": "unbound",
            "keys": "alt+down"
        },
        {
            "command": "unbound",
            "keys": "alt+left"
        },
        {
            "command": "unbound",
            "keys": "alt+right"
        },
        {
            "command": "unbound",
            "keys": "alt+up"
        },
        {
            "command": "unbound",
            "keys": "alt+j"
        },
        {
            "command": "unbound",
            "keys": "alt+h"
        },
        {
            "command": "unbound",
            "keys": "alt+l"
        },
        {
            "command": "unbound",
            "keys": "alt+k"
        },
        {
            "command":
            {
                "action": "openSettings"
            },
            "keys": "ctrl+/"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "up"
            },
            "keys": "ctrl+k"
        },
        {
            "command":
            {
                "action": "newTab"
            },
            "keys": "alt+t"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "down"
            },
            "keys": "ctrl+j"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "right"
            },
            "keys": "ctrl+l"
        },
        {
            "command":
            {
                "action": "moveFocus",
                "direction": "left"
            },
            "keys": "ctrl+h"
        }
    ],
